{
    "name": "popphp/pop-console",
    "description": "Pop Console Component for Pop PHP Framework",
    "keywords": [
        "console",
        "cli",
        "console application",
        "cli application",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-console",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "popphp/popphp": "^4.3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Console\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2.x-dev"
        }
    }
}

