{
    "name": "popphp/pop-console",
    "description": "Pop Console Component for Pop PHP Framework",
    "keywords": [
        "console",
        "cli",
        "console application",
        "cli application",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "popphp/popphp": "^3.2.2",
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Console\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}

